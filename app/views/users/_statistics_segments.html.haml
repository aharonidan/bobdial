- @charts_data.each_with_index do |chart, index|
  - if index % 2 == 0
    = column_chart chart
  - else
    = column_chart chart, colors: ["#c0392b"]
  %table.table
    %tbody
      - @groups_data[index].each do |category, users|
        %tr.no-border-bottom
          %td
            %label.label= category
          %td
            = users.join(', ')
